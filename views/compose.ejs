<%- include('./partials/header'); %>

<h1>
    Compose
</h1>
<form action="/compose" method="post" class="form-group">
    <div class="form-group">
        <label for="text"><h3>Title:</h3></label>
    <input type="text" class="form-control" name="title">
    </div>
    
    <div class="form-group">
        <label for="text"><h3>Post:</h3></label>
        <textarea name="content" class="form-control" id="message" cols="30" rows="10"></textarea>
        
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
    

</form>



<%- include('./partials/footer'); %>